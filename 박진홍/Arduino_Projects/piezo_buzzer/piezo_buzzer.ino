#define C 262#define D 294#define E 330#define F 349#define G 392#define A 440#define B 494int piezo = 3;int sw = 13;int nNum = 16;int nTones[16] = {C,C,G,G,A,A,G,G,F,F,E,E,D,D,C,C};void setup() {  pinMode(piezo, OUTPUT);  pinMode(sw, INPUT_PULLUP);}void loop() {  if(digitalRead(sw)==LOW)   {    for(int i=0;i<16;++i)    {      tone(piezo, nTones[i]);      delay(500);    }    noTone(piezo);  }}